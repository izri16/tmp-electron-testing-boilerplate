{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "declaration": false,
    "declarationMap": false,
    "strict": true,
    "pretty": true,
    "sourceMap": true,
    "noUnusedLocals": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "allowJs": false,
    "skipLibCheck": true,
    "esModuleInterop": true
  }
}

